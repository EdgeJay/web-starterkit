{
  "name": "web-starterkit",
  "version": "0.5.0",
  "description": "Template project to kickstart new web projects",
  "scripts": {
    "build:html": "curl localhost:6150 -o ./src/client/assets/index.html",
    "build:sw": "cp ./node_modules/workbox-sw/build/importScripts/workbox-sw.prod.v2.1.2.js ./dist",
    "build:analyse": "ANALYSE_BUILD_MODE=true NODE_ENV=production ./node_modules/.bin/webpack",
    "build": "chmod +x ./scripts/build.sh && ./scripts/build.sh",
    "dev:server": "nodemon -L",
    "lint:server": "eslint -c ./.eslintrc.js ./index.js \"./src/server/**\"",
    "start": "node --harmony index.js",
    "preinstall": "chmod +x ./scripts/preinstall.sh && ./scripts/preinstall.sh",
    "postinstall": "chmod +x ./scripts/postinstall.sh && ./scripts/postinstall.sh",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": "8.6.0"
  },
  "keywords": [
    "web",
    "boilerplate",
    "starter",
    "react",
    "react-fiber",
    "react16",
    "koa",
    "koa2",
    "milligram",
    "styled-components"
  ],
  "author": "Wu Huijie",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-styled-components": "^1.3.0",
    "babel-plugin-system-import-transformer": "^3.1.0",
    "babel-plugin-transform-assets-import-to-string": "^1.0.1",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "classnames": "^2.2.5",
    "dotenv": "^4.0.0",
    "dustjs-linkedin": "^2.7.5",
    "fetch-ponyfill": "^4.1.0",
    "koa": "^2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-csrf": "3.x",
    "koa-helmet": "^3.2.0",
    "koa-router": "^7.2.1",
    "koa-session": "^5.5.0",
    "koa-static": "^4.0.1",
    "koa-views": "^6.0.2",
    "localforage": "^1.5.5",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-async-bootstrapper": "^1.1.2",
    "react-async-component": "^1.0.2",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^3.1.0-rc.1",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "serialize-javascript": "^1.4.0",
    "styled-components": "^2.1.2",
    "workbox-sw": "^2.1.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.2.0",
    "compression-webpack-plugin": "^1.0.1",
    "eslint": "^4.7.2",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "file-loader": "^0.11.2",
    "koa-webpack-middleware": "^1.0.7",
    "nodemon": "^1.12.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.8.2",
    "webpack-hot-middleware": "^2.19.1",
    "workbox-webpack-plugin": "^2.1.2"
  }
}
