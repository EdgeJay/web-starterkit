{
  "presets": [
    ["env", {
      "targets": {
        "node": "current"
      }
    }],
    "react"
  ],
  "plugins": [
    ["styled-components", {
      "ssr": true
    }],
    "transform-react-jsx",
    "transform-es2015-spread",
    "transform-object-rest-spread",
    ["transform-assets-import-to-string", {
      "extensions": [".html", ".ico", ".png", ".jpg", ".jpeg", ".svg", ".gif", ".otf", ".ttf", ".json"],
      "baseDir": "/assets",
      "baseUri": ""
    }],
    "system-import-transformer"
  ]
}